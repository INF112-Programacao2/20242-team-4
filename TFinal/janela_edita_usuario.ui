   <string notr="true">*{
	background-color: #4A90E2;
}

QLabel{
	font-family: &quot;Segoe UI&quot;;
	font-weight: bold;
	font-size: 18px;
    color: #fff;
}

QLineEdit{
    background: #ffffff;
    border: 2px solid #4A90E2;
    border-radius: 8px;
    padding: 5px 10px;

    font-family: &quot;Segoe UI&quot;;
    font-weight: bold;
    font-size: 16px;
    color: #333333;
}

QLineEdit:focus{
    border-color: #53A3FF;
    background: #E6F7FF; 
}

QLineEdit:disabled {
    background: #f0f0f0;
    border: 2px solid #cccccc;
    color: #999999;
}

QRadioButton {
    background: #ffffff;
    border: 2px solid #4A90E2;
    border-radius: 8px;
    padding: 5px 10px;

    font-family: &quot;Segoe UI&quot;;
    font-weight: bold;
    font-size: 14px;
    color: #000000;
}

QRadioButton:checked {
    background: #4A90E2; 	/* Fundo azul quando selecionado */
    border-color: #53A3FF; 	/* Borda com azul mais claro */
    color: #FFFFFF;					 /* Texto branco */
}

QRadioButton:hover {
    border-color: #53A3FF;	 /* Borda azul claro ao passar o mouse */
    background: #E6F7FF;		 /* Fundo azul claro */
    color: #1D08FF;					 /* Texto branco */
}

QPushButton{
    background: #4A90E2;
    border: 2px solid #fff;
    border-radius: 14px;
    padding: 10px 20px;

	font-family: &quot;Segoe UI&quot;;
	font-weight: bold;
	font-size: 18px;
    color: #fff;
}

QPushButton:hover{
    background: #53A3FF;
    border: 2px solid #fff;
    border-radius: 12px;
    padding: 10px 20px;
}

QDateEdit {
    background: #ffffff;
    border: 2px solid #4A90E2;
    border-radius: 8px;
    padding: 5px 10px;

    font-family: &quot;Segoe UI&quot;;
    font-weight: bold;
    font-size: 16px;
    color: #333333;
}

QDateEdit:focus {
    border-color: #53A3FF;
    background: #E6F7FF;
}

QDateEdit::drop-down {
    border: none;
    width: 20px;
    background: #4A90E2;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}

QDateEdit::drop-down:hover {
    background: #53A3FF;
}

QDateEdit:disabled {
    background: #f0f0f0;
    border: 2px solid #cccccc;
    color: #999999;
}

QDateEdit::down-arrow {
    width: 10px;
    height: 10px;
}</string>
