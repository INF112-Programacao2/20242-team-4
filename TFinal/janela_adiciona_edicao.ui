    border-radius: 12px;
    padding: 10px 20px;
}

QDateEdit {
    background: #ffffff;
    border: 2px solid #4A90E2;
    border-radius: 8px;
    padding: 5px 10px;

    font-family: &quot;Segoe UI&quot;;
