    font-size: 16px;
    color: #333333;
}

QDateEdit:focus {
    border-color: #53A3FF;
    background: #E6F7FF;
}

QDateEdit::drop-down {
    border: none;
    width: 20px;
    background: #4A90E2;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}

QDateEdit::drop-down:hover {
    background: #53A3FF;
}

QDateEdit:disabled {
    background: #f0f0f0;
    border: 2px solid #cccccc;
    color: #999999;
}

QDateEdit::down-arrow {
    width: 10px;
    height: 10px;
